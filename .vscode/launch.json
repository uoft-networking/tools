{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Remote Attach",
            "type": "python",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 5678
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ],
            "justMyCode": true
        },
        {
            "name": "Debug Tests",
            "type": "python",
            "request": "launch",
            "purpose": [
                "debug-test"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "VSCODE_DEBUGGER": "1"
            },
        },
        {
            "name": "Python: Nautobot UTSC_Debug",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/nautobot-server",
            "args": [
                "utsc_debug",
            ],
            "django": true,
            "justMyCode": false,
            "cwd": "${workspaceFolder}/projects/nautobot",
            "env": {
                "PYDEBUG": "1",
                "VSCODE_DEBUGGER": "1"
            },
        },
        {
            "name": "Python: Nautobot Dev",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/nautobot-server",
            "args": [
                "runserver",
                "--noreload"
            ],
            "django": true,
            "justMyCode": false,
            "cwd": "${workspaceFolder}/projects/nautobot",
            "env": {
                "VSCODE_DEBUGGER": "1"
            },
        },
        {
            "name": "Python: Nautobot Worker",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/nautobot-server",
            "args": [
                "celery",
                "worker",
                "--loglevel",
                "DEBUG",
            ],
            "django": true,
            "justMyCode": false,
            "cwd": "${workspaceFolder}/projects/nautobot",
            "env": {
                "PYDEBUG": "1",
                "VSCODE_DEBUGGER": "1"
            },
        },
        {
            "name": "Python: Select module to debug",
            "type": "python",
            "request": "launch",
            "module": "uoft_core",
            "console": "integratedTerminal",
            "env": {
                "PYDEBUG": "1"
            },
            "justMyCode": false
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYDEBUG": "1"
            },
            "justMyCode": false
        },
    ]
}