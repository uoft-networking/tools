    "version": "0.2.0",
    "configurations": [

        {
            "name": "Debug Tests",
            "type": "python",
            "request": "test",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "VSCODE_DEBUGGER": "1"
            },
        },
        {
            "name": "Python: Module: __main__",
            "type": "python",
            "request": "launch",
            "module": "utsc.__main__",
            "console": "integratedTerminal",
            "env": {
                "PYDEBUG": "1"
            },
            "justMyCode": false
        },
        {
            "name": "Python: Module: utsc.switchconfig.__main__",
            "type": "python",
            "request": "launch",
            "module": "utsc.switchconfig.__main__",
            "console": "integratedTerminal",
            "env": {
                "PYDEBUG": "1"
            },
            "justMyCode": false
        },
        {
            "name": "Python: Module: utsc.scripts.__main__",
            "type": "python",
            "request": "launch",
            "module": "utsc.scripts.__main__",
            "console": "integratedTerminal",
            "env": {
                "PYDEBUG": "1"
            },
            "justMyCode": false
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYDEBUG": "1"
            },
            "justMyCode": false
        },
    ]
}
